# Misskey-rs Environment Variables
# Copy this file to .env and modify as needed

# Server
MISSKEY_URL=http://localhost:3000
MISSKEY_PORT=3000
MISSKEY_BIND=0.0.0.0

# Database
DATABASE_URL=postgres://misskey:misskey@localhost:5432/misskey

# Redis
REDIS_URL=redis://localhost:6379

# Logging
RUST_LOG=misskey=debug,tower_http=debug

# Optional: Meilisearch
# MEILISEARCH_HOST=http://localhost:7700
# MEILISEARCH_API_KEY=

# Optional: Object Storage (S3-compatible)
# S3_ENDPOINT=
# S3_REGION=
# S3_BUCKET=
# S3_ACCESS_KEY=
# S3_SECRET_KEY=
